{
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.minTriggerInputLength": 2,
  "suggest.removeDuplicateItems": false,
  "[vim]": {
    "diagnostic.enable": false
  },
  "explorer.keyMappings.global": {
    "<cr>": [
      "wait",
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ]
  }
}
