{
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.minTriggerInputLength": 2,
  "suggest.removeDuplicateItems": false,
  "signature.hideOnTextChange": true,
  "[vim]": {
    "diagnostic.enable": true
  },
  "explorer.keyMappings.global": {
    "<cr>": [
      "wait",
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    "s": "open:vsplit",
    "i": "open:split",
    "C": "cd",
    "o": [
      "wait",
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ]
  },
  "Lua.telemetry.enable": true,
  "java.templates.fileHeader": [
    "/**",
    " * ${type_name}",
    " * @author ${user}",
    " */"
  ],
  "java.templates.typeComment": [
    "/**",
    " * ${type_name}",
    " * @author ${user}",
    " */"
  ],
  "texlab.chktex.onOpenAndSave": true,
  "texlab.chktex.onEdit": true,
  "texlab.forwardSearch.executable": "zathura",
  "texlab.forwardSearch.args": ["--synctex-forward", "%l:1:%f", "%p"],
  "texlab.build.forwardSearchAfter": true
}
